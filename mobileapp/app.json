{
  "expo": {
    "name": "reading-wrapped-mobile",
    "slug": "reading-wrapped-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./resources/logo_v1.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Allow $(PRODUCT_NAME) to access your camera"
      },
      "bundleIdentifier": "com.anonymous.readingwrappedmobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA"
      ],
      "package": "com.anonymous.readingwrappedmobile"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#232323",
          "image": "./resources/logo_v1.png",
          "dark": {
            "image": "./resources/logo_v1.png",
            "backgroundColor": "#808080"
          },
          "imageWidth": 200
        }
      ],
      [
        "react-native-vision-camera",
        {
          "enableCodeScanner": true
        }
      ]
    ]
  }
}
